#include <stdio.h>
#include <string.h>

int main() {
    int n;
    char files[20][20], name[20];

    printf("Enter number of files: ");
    scanf("%d", &n);

    printf("Enter file names:\n");
    for (int i = 0; i < n; i++) {
        scanf("%s", files[i]);
    }

    printf("Enter file to search: ");
    scanf("%s", name);

    for (int i = 0; i < n; i++) {
        if (strcmp(files[i], name) == 0) {
            printf("File found.\n");
            return 0;
        }
    }
    printf("File not found.\n");
}
File found.
